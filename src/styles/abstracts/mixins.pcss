$screen-md: 600px;
$screen-lg: 900px;
$screen-xl: 1100px;
@define-mixin fallback-clamp $property , $sm, $md, $lg{
  $(property): $(sm);
  @mixin screen-size-md {
    $(property): $(md)
  }

  @mixin screen-size-lg {
    $(property): $(lg)
  }
}

@define-mixin screen-size-md {
  @media (min-width: $screen-md) {
    @mixin-content;
  }
}

@define-mixin screen-size-lg {
  @media (min-width: $screen-lg) {
    @mixin-content;
  }
}

@define-mixin screen-size-xl {
  @media (min-width: $screen-xl) {
    @mixin-content;
  }

}